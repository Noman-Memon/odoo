<odoo>

    <!--    in action tag module must be used of that module which action Id we have to define-->
    <!--    e.g..(tutorial_project.rest_staff_action_id or-->
    <!--    mongodb_integration.data_collection_action_id)-->

    <menuitem id="restaurant_main_menu" name="Restaurant"/>
    <menuitem id="restaurant_sub_menu" name="OurTeam" parent="tutorial_project.restaurant_main_menu" sequence="10"/>
    <menuitem id="restaurant_all_records_sub_menu" name="All Records" parent="tutorial_project.restaurant_sub_menu"
              sequence="1"
              action="tutorial_project.rest_staff_action_id"/>
    <menuitem id="restaurant_male_records_sub_menu" name="Male Records" parent="tutorial_project.restaurant_sub_menu"
              sequence="2"
              action="tutorial_project.rest_staff_male_action_id"/>
    <menuitem id="restaurant_female_records_sub_menu" name="Female Records"
              parent="tutorial_project.restaurant_sub_menu"
              action="tutorial_project.rest_staff_female_action_id" sequence="3"/>
    <!--    # if we want to call wizard by menu then-->
    <menuitem id="restaurant_update_wizard_menu" name="Open Wizard"
              parent="tutorial_project.restaurant_main_menu"
              action="tutorial_project.rest_staff_wizard_action_id" sequence="11"/>
    <!--    if we want to call any function by menuitem-->
    <menuitem id="restaurant_function_call_by_menu" name="Function Call By Menu"
              parent="tutorial_project.restaurant_main_menu"
              action="tutorial_project.function_call_by_menu" sequence="12"/>
    <menuitem id="restaurant_department_menu" name="Department"
              parent="tutorial_project.restaurant_main_menu"
              action="tutorial_project.rest_staff_department_action_id" sequence="13"/>
